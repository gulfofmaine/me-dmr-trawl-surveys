        <addAttributes>
            <att name="long_name">SAMPLE LENGTH</att>
            <att name="description">Lobster carapace length in mm</att>
            <att name="units">mm</att>
        </addAttributes>
    </dataVariable>
    <!-- ADD HERE --> 
